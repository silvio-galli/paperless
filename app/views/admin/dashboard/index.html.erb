<h1>Admin Dashboard</h1>
<%= link_to "New User", new_user_registration_path, class: 'btn btn-primary btn-sm' %>
<div class="row">
  <div class="col-md-6">
    <h3>Users List</h3>
    <ul>
      <% @users.each do |user| %>
        <li>
          <%= user.id %> | <%= user.name %>
        </li>
      <% end %>
    </ul>
  </div>
</div>
